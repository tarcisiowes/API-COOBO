{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "consistent-return":"off",
        "default-case":"off",
        "new-cap":"off",
        "prefer-const":"off"
    }
}
